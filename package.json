  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy": "node tools/copy-package.js && node tools/cleanup.js",
    "bundle": "rm -rf publish && cp -a src/app/lib publish",
    "copy-style": "cp src/theme.scss publish/theme.scss && cp src/palette.scss publish/palette.scss",
    "prepublish": "npm run bundle && npm run copy && npm run copy-style"
  },
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/common": "^4.3.6",
