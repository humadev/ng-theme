    "@angular/core": "^4.3.6",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/material": "^2.0.0-beta.10",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/router": "^4.3.6",
    "apollo-angular": "^0.13.1",
    "apollo-client": "^1.9.2",
    "core-js": "^2.4.1",
    "graphql-tag": "^2.4.2",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.6"
